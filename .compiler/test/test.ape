# main Todo object
Todo -> Collection
  message -> Text
    min_length: 4
    max_length: 16
  ratio -> Float?
    max: 99.99
  score -> .NumEnum
  user_info -> Collection
    username -> .Username
    first_name -> Text?

# user utf-8 alphanumeric identifier
Username -> Text
  min_length: 3
  max_length: 16

# content of todo
Message -> Text

NumEnum -> Enum
  BLACK  0
  BLUE   1
  YELLOW 2

TextEnum -> Enum
  UNDEFINED
  DIFFERENT TESTDIFF
  SAME SAME
  TESTLONGALIGNMENT TEST

TodoResult -> Union
  .Username    |

RecentTodos -> Array
  .Todo
