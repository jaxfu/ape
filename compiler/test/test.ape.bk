# main Todo object
Todo -> .Collection?
	message -> Text # inline comment
		min_length: 4
		max_length: 16
		alpha:      true
	ratio -> Float?
		max: 99.99
	score -> .NumEnum
	user_info -> Collection
		username -> .Username
		first_name -> Text?

Message -> Text

NumEnum -> Enum
	BLACK  0
	BLUE   1
	YELLOW 2

TextEnum -> Enum
	UNDEFINED
	DIFFERENT         TESTDIFF
	SAME              SAME
	TESTLONGALIGNMENT TEST

TodoResult -> Union
	.Username    |
	.Message

RecentTodos -> Array
	.Todo
